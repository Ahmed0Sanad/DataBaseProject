@model Dictionary<OfferedCourse,int>
@{
    ViewData["Title"] = "Grade";
}

<h1 class="text-center" style="    margin-left: -40px;">Grades</h1>




@if (Model.Any())
{
    <table id="employeeTable" class="table table-striped table-bordered text-center">
        <thead class="table-dark">
            <tr>
                <th> Id</th>
                <th>Course Name</th>
                <th>Grade</th>
    
       
            </tr>
        </thead>
        <tbody>
            @foreach (var e in Model)
            {
                <tr>
                    <td>@e.Key.CourseId</td>
                    <td>@e.Key.Course.Title</td>
                    <td>@e.Value</td>


                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h2 class="text-danger text-center">There are no Courses.</h2>
}
<a style="    margin-left: 48%; transform: translateX(-50%);" class="btn btn-secondary mt-4" asp-area="" asp-controller="Student" asp-action="Index">Back To List</a>