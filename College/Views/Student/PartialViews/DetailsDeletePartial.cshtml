@model Student
<div class="form-group mb-3">
    <label asp-for="Name" class="form-label">Student Name</label>
    <input type="text" asp-for="Name" id="Name" class="form-control" readonly />

</div>

<div class="form-group mb-3">
    <label asp-for="Year" class="form-label">Student Year</label>
    <input type="text" asp-for="Year" id="Num" class="form-control" readonly />

</div>

<div class="form-group mb-3">
    <label asp-for="Age" class="form-label">Age</label>
    <input type="text" asp-for="Age" class="form-control" readonly />

</div>
<div class="form-group mb-3">
    <label asp-for="Email" class="form-label">Email</label>
    <input type="text" asp-for="Email" class="form-control" readonly />

</div>
<div class="form-group mb-3">
    <label asp-for="Phone" class="form-label">Phone</label>
    <input type="text" asp-for="Phone" class="form-control" readonly />

</div>
<div class="form-group mb-3">
    <label asp-for="Address" class="form-label">Address</label>
    <input type="text" asp-for="Address" class="form-control" readonly />

</div>

