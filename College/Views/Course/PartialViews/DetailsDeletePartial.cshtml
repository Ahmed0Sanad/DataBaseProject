@model Course
<div>
    <div class="form-group mb-3">
        <label asp-for="Title" class="form-label">Course Title</label>
        <input type="text" asp-for="Title" id="Title" class="form-control " readonly />

    </div>

    <div class="form-group mb-3">
        <label asp-for="Price" class="form-label">Price</label>
        <input type="text" asp-for="Price" id="Num" class="form-control" readonly />

    </div>

    <div class="form-group mb-3">
        <label asp-for="Syllabus" class="form-label">Syllabus</label>
        <input type="text" asp-for="Syllabus" id="Code" class="form-control" readonly />

    </div>

    <table class ="table table-striped table-bordered text-center">
        <thead class ="table-dark">
            <tr>
                <th>PreQuestes</th>
            </tr>
        </thead>
    <tbody>
            @foreach (Course pre in ViewBag.Pre)
            {
                <tr>
                    <td> @pre.Id __  @pre.Title  </td>
                </tr>
         
            }
        </tbody>
</table>


</div>
